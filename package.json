{
  "name": "turkify",
  "version": "0.1.0",
  "description": "Türkçe karakter normalizasyon paketi - SEO ve URL'ler için",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "turkish",
    "türkçe",
    "normalize",
    "slugify",
    "url",
    "seo",
    "character",
    "unicode"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
